datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Url {
  id          Int      @id @default(autoincrement())
  shortCode   String   @unique
  originalUrl String
  createdAt   DateTime @default(now())
  expireAt    DateTime?
}
